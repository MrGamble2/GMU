#--------------------------------------------------------
#  Choose  a  compiler  
#-------------------------------------------------------- 
CC = gcc

#--------------------------------------------------------
#  Set  your  source  file  and  your  target  executable    
#--------------------------------------------------------
SRC = lab6_sgamble2.c
TARGET = lab6_sgamble2  

all: $(TARGET)

#--------------------------------------------------------
#  Compile  the  program    
#--------------------------------------------------------
$(TARGET): $(SRC)
	$(CC)  -o  $@  $(SRC)  

#--------------------------------------------------------
#  Clean  up  (remove  the  executable)  
#--------------------------------------------------------
clean:
	rm  -f  $(TARGET)  
